<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kean.memgameredo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserChoiceActivity" />
        <activity android:name=".MemoryGameActivity" />
        <activity android:name=".HighScoreActivity" />
        <activity android:name=".UserChoiceHighScoreActivity" />
        <activity android:name=".CreateHighScoreActivity"></activity>
    </application>

</manifest>